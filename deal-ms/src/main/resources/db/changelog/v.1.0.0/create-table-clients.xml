<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.6.xsd">

    <changeSet id="create-table-clients" author="KonchalovMaxim">
        <createTable tableName="t_clients">
            <column name="id" type="bigint" autoIncrement="true">
                <constraints nullable="false" primaryKey="true"/>
            </column>
            <column name="account" type="varchar(255)"/>
			<column name="birthdate" type="date"/>
			<column name="dependent_amount" type="int"/>
			<column name="email" type="varchar(255)"/>
			<column name="first_name" type="varchar(255)"/>
			<column name="last_name" type="varchar(255)"/>
			<column name="marital_status" type="varchar(255)"/>
			<column name="middle_name" type="varchar(255)"/>
			<column name="employment_id" type="bigint"/>
			<column name="passport_id" type="bigint"/>
        </createTable>
    </changeSet>

</databaseChangeLog>